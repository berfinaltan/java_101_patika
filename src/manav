import java.util.Scanner;




public class manav   {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);

        double armut=2.14;
        double elma=3.67;
        double domates=1.11;
        double muz=0.95;
        double patlican=5.00;


        System.out.print("Armut kaç kilo: " );
        double armutKilo=in.nextDouble();

        System.out.print("Elma kaç kilo: ");
        double elmaKilo=in.nextDouble();

        System.out.print("Domates kaç kilo: ");
        double domatesKilo=in.nextDouble();

        System.out.print("Muz kaç kilo: ");
        double muzKilo=in.nextDouble();

        System.out.print("Patlican kaç kilo: ");
        double patlicanKilo=in.nextDouble();

        double toplam= (armut * armutKilo) + (elma * elmaKilo) + (domates * domatesKilo) + (muz * muzKilo) + (patlican * patlicanKilo);
        System.out.println("Toplam: " + toplam);





